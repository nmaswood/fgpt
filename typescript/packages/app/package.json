{
  "name": "@fgpt/app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev::server": "NODE_OPTIONS='--inspect' next dev",
    "dev::watch": "yarn tsc --noEmit --incremental --watch",
    "dev": "concurrently --names \"NEXT,WATCH\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn dev::server\" \"yarn dev::watch\"",
    "build": "next build",
    "start": "next start",
    "test": "yarn vitest src/tests",
    "fix": "yarn fix::lint; yarn fix::format",
    "fix::lint": "eslint . --fix",
    "fix::format": "prettier --write pages/",
    "check": "yarn check::lint && yarn check::format",
    "check::lint": "eslint . --ext .ts",
    "check::format": "prettier --check .",
    "clean": "yarn clean::dist",
    "clean::dist": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fgpt/precedent-iso": "workspace:^",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "next": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.3.1",
    "@types/eslint": "^8.37.0",
    "@types/node": "18.16.2",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
