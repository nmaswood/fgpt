{
  "name": "@fgpt/app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "-dev::server": "NODE_OPTIONS='--inspect' next dev",
    "dev::server": "NODE_OPTIONS='--inspect' next dev",
    "dev::watch": "yarn tsc --noEmit --incremental --watch",
    "dev": "concurrently --names \"NEXT,WATCH\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn start\" \"yarn dev::watch\"",
    "build": "next build",
    "start": "next start",
    "test": "yarn vitest src/tests",
    "fix": "yarn fix::lint; yarn fix::format",
    "fix::lint": "eslint . --fix",
    "fix::format": "prettier --write pages/",
    "check": "yarn check::lint && yarn check::format",
    "check::lint": "eslint . --ext .ts",
    "check::format": "prettier --check .",
    "clean": "yarn clean::dist",
    "clean::dist": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^2.6.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fgpt/precedent-iso": "workspace:^",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.134",
    "@mui/material": "^5.13.5",
    "@mui/x-data-grid": "^6.8.0",
    "@uppy/core": "^3.2.1",
    "@uppy/dashboard": "^3.4.1",
    "@uppy/drag-drop": "^3.0.2",
    "@uppy/file-input": "^3.0.2",
    "@uppy/progress-bar": "^3.0.2",
    "@uppy/react": "^3.1.2",
    "@uppy/xhr-upload": "^3.3.0",
    "next": "^13.4.6",
    "next-http-proxy-middleware": "^1.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.6",
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  }
}
