{
  "name": "@fgpt/app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "-dev::server": "NODE_OPTIONS='--inspect' next dev",
    "dev::server": "NODE_OPTIONS='--inspect' next dev",
    "watch": "yarn tsc --noEmit --incremental --watch",
    "t-check": "yarn tsc --noEmit ",
    "dev": "concurrently --names \"NEXT,WATCH\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn start\" \"yarn watch\"",
    "build": "next build",
    "start": "next start",
    "test": "yarn vitest src/tests",
    "fix": "yarn fix::lint; yarn fix::format",
    "fix::lint": "eslint . --fix",
    "fix::format": "prettier --write pages/",
    "check": "yarn check::lint && yarn check::format",
    "check::lint": "eslint . --ext .ts",
    "check::format": "prettier --check .",
    "clean": "yarn clean::dist",
    "clean::dist": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.3.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fgpt/precedent-iso": "workspace:^",
    "@mui/icons-material": "^5.14.19",
    "@mui/joy": "^5.0.0-beta.16",
    "@mui/material": "^5.14.19",
    "@mui/x-data-grid": "^6.18.2",
    "@uppy/aws-s3": "^3.5.0",
    "@uppy/core": "^3.7.1",
    "@uppy/dashboard": "^3.7.1",
    "@uppy/drag-drop": "^3.0.3",
    "@uppy/file-input": "^3.0.4",
    "@uppy/progress-bar": "^3.0.4",
    "@uppy/react": "^3.2.1",
    "@uppy/xhr-upload": "^3.5.0",
    "fecha": "^4.2.3",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.0",
    "swr": "^2.2.4",
    "typescript-cookie": "^1.0.6",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/eslint": "^8.44.8",
    "@types/node": "^20.10.1",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
